{
  "name": "casscms",
  "version": "0.1.0",
  "description": "一个使用 react 开发的 CMS、博客，next.js 框架做服务器端渲染，koa2 实现 RESTFul 风格后台接口",
  "keywords": [
    "react",
    "next",
    "redux",
    "nodejs",
    "koa2",
    "mongodb",
    "webpack",
    "restful"
  ],
  "author": "silinchen <silinccc@gmail.com>",
  "license": "MIT",
  "scripts": {
    "next:dev": "next dev",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "commit": "npx git-cz",
    "cz:init": "commitizen init cz-conventional-changelog --yarn --dev --exact",
    "initData": "node server/initData.js"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.26.6",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "cross-env": "^6.0.3",
    "crypto-js": "^3.1.9-1",
    "gm": "^1.23.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-json-error": "^3.1.2",
    "koa-jwt": "^3.6.0",
    "koa-parameter": "^3.0.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "less": "^3.10.3",
    "moment": "^2.24.0",
    "mongoose": "^5.7.13",
    "next": "9.3.2",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^4.0.1",
    "path-to-regexp": "^6.1.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "^7.1.3",
    "react-toastify": "^5.5.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "screenfull": "^5.0.0",
    "spark-md5": "^3.0.0",
    "webp-converter": "^2.2.3"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "nodemon": "^1.19.4",
    "null-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.8",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
